{
  "name": "@laufire/utils",
  "version": "3.0.0-0",
  "description": "A simple set of utility libraries to ease development.",
  "main": "./index.js",
  "scripts": {
    "copy": "cp ./package.json ./ReadMe.md ./LICENSE ./dist/",
    "build": "rm -rf ./dist && babel --ignore '**/*.spec.js' -d ./dist ./src && npm run copy",
    "dev": "npm run build && cd ./dist && npm link && cd ..",
    "test": "jest --no-cache",
    "test-dev": "retryCount=10 npm run test -- --collectCoverage=false",
    "test-ci": "npm run test -- --coverage",
    "lint": "eslint .",
    "prepareDist": "npm run lint && npm test && npm run build",
    "dist": "npm run prepareDist && cd dist && npm publish --ignore-scripts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Laufire/js-utils.git"
  },
  "bugs": {
    "url": "https://github.com/Laufire/js-utils/issues"
  },
  "homepage": "https://github.com/Laufire/js-utils#readme",
  "keywords": [
    "utils",
    "collection",
    "random"
  ],
  "author": "laufire technologies",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@laufire/utils": "^2.9.2",
    "acorn": "^6.4.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^5.16.0",
    "jest": "^26.6.3",
    "node-pre-gyp": "^0.13.0",
    "regenerator-runtime": "^0.13.7"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**/*.js"
    ],
    "coverageDirectory": "./.coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "testRegex": "src/.*\\.spec\\.js$"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0"
  }
}
