{
  "name": "@laufire/utils",
  "version": "1.3.0",
  "description": "A simple set of utility libraries to ease development.",
  "main": "./index.js",
  "scripts": {
    "copy": "cp ./package.json ./ReadMe.md ./LICENSE ./dist/",
    "build": "babel --ignore '**/*.spec.js' -d ./dist ./src && npm run copy",
    "dev": "node -e 'require(\"./dist/index.js\").main()'",
    "test": "jest --no-cache",
    "lint": "eslint .",
    "prepublishOnly": "echo \"Error: Don't run 'npm publish' in root. Use 'npm run dist' instead.\" && exit 1",
    "prepareDist": "npm run lint && npm test && rm -rf ./dist && npm run build",
    "dist": "npm run prepareDist && cd dist && npm publish --ignore-scripts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Laufire/js-utils.git"
  },
  "bugs": {
    "url": "https://github.com/Laufire/js-utils/issues"
  },
  "homepage": "https://github.com/Laufire/js-utils#readme",
  "keywords": [
    "utils",
    "collection",
    "random"
  ],
  "author": "laufire technologies",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.5.5",
    "acorn": "^6.4.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "eslint": "^5.16.0",
    "fsevents": "^2.0.7",
    "jest": "^24.9.0",
    "node-pre-gyp": "^0.13.0",
    "regenerator-runtime": "^0.13.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**/*.js"
    ],
    "coverageDirectory": "./.coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "dependencies": {}
}
