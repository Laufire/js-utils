{
  "name": "@laufire/utils",
  "version": "0.0.14",
  "description": "A simple set of utility libraries to ease development.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest --no-cache",
    "lint": "eslint .",
    "build": "babel --presets es2015 -d ./dist ./src",
    "prepublishOnly": "npm test && npm run build && cp ./package*.json ./dist/",
    "publish": "npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Laufire/js-utils.git"
  },
  "bugs": {
    "url": "https://github.com/Laufire/js-utils/issues"
  },
  "homepage": "https://github.com/Laufire/js-utils#readme",
  "keywords": [
    "utils",
    "collection",
    "random"
  ],
  "author": "laufire technologies",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.16.0",
    "jest": "^24.7.1",
    "regenerator-runtime": "^0.13.2"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./.jest-coverage",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
